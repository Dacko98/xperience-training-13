@model IEnumerable<XperienceAdapter.Dtos.SocialLink>
@inject CMS.Core.ILocalizationService LocalizationService

<h5 class="white-text">@LocalizationService.GetString("ViewComponent.SocialLink.FollowUs")</h5>
<p>
    @if (Model != null && Model.Any())
    {
        foreach (var socialLink in Model)
        {
            <a href="@Html.Raw(socialLink.Url)">
                <img src="@Html.Raw(socialLink.IconUrl)" alt="@Html.DisplayFor(model => socialLink.Name)" />
            </a>
        }
    }
</p>