@model IEnumerable<XperienceAdapter.SocialLink>

<h5 class="white-text">Follow us</h5>
<p>
    @if (Model != null && Model.Any())
    {
        foreach (var socialLink in Model)
        {
            <a href="@Html.Raw(socialLink.Url)">
                <img src="@Html.Raw(socialLink.IconUrl)" alt="@Html.DisplayFor(model => socialLink.Name)" />
            </a>
        }
    }
</p>